{
    "compilerOptions": {
        "incremental": true,
        "target": "es2020",
        "module": "ESNext",
        "declaration": true,
        "sourceMap": true,
        "strict": true,
        "moduleResolution": "nodenext",
        "esModuleInterop": true,
        "skipLibCheck": true,
        "forceConsistentCasingInFileNames": true,
        "composite": true
    }
}